{
  "name": "@expect.tv/server",
  "version": "0.1.0",
  "private": true,
  "description": "The server component of Expect TV",
  "homepage": "https://github.com/avidenie/expect.tv-server#readme",
  "bugs": {
    "url": "https://github.com/avidenie/expect.tv-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avidenie/expect.tv-server.git"
  },
  "author": "Adrian Videnie <avidenie@gmail.com>",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "per-env",
    "start:development": "nodemon src/index.ts",
    "start:production": "NODE_PATH=./build node build/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "apollo-datasource-rest": "~0.8.1",
    "apollo-server": "~2.12.0",
    "dotenv": "~8.2.0",
    "graphql": "~15.0.0",
    "luxon": "~1.23.0",
    "per-env": "~1.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.3",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-resolvers": "1.13.3",
    "@types/dotenv": "~8.2.0",
    "@types/node": "^13.13.2",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-prettier": "~3.1.3",
    "global-agent": "~2.1.8",
    "husky": "~4.2.5",
    "nodemon": "~2.0.3",
    "prettier": "~2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts",
    "ignore": [
      "*.test.ts"
    ],
    "exec": "node -r global-agent/bootstrap -r tsconfig-paths/register -r ts-node/register"
  }
}
